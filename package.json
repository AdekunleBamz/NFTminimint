{
  "name": "nftminimint",
  "version": "2.0.0",
  "description": "A comprehensive NFT minting platform with advanced features - FREE MINTING!",
  "author": "Adekunle Bamz",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:gas": "npx hardhat test test/Gas.test.js",
    "test:security": "npx hardhat test test/Security.test.js",
    "test:integration": "npx hardhat test test/Integration.test.js",
    "deploy": "npx hardhat run scripts/deploy-all.js",
    "deploy:sepolia": "npx hardhat run scripts/deploy-all.js --network sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy-all.js --network mainnet",
    "link": "npx hardhat run scripts/link-contracts.js",
    "verify": "npx hardhat run scripts/verify-contracts.js",
    "stats": "npx hardhat run scripts/collection-stats.js",
    "airdrop": "npx hardhat run scripts/airdrop.js",
    "coverage": "npx hardhat coverage"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.0.0",
    "dotenv": "^17.2.3",
    "hardhat": "^2.19.0"
  },
  "license": "MIT",
  "keywords": [
    "nft",
    "ethereum",
    "solidity",
    "erc721",
    "minting",
    "free",
    "hardhat",
    "openzeppelin",
    "web3",
    "blockchain",
    "smart-contracts",
    "base"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AdekunleBamz/NFTminimint"
  }
}
